{
  "name": "shintack/laravel-logger",
  "description": "Laravel Logger Package",
  "keywords": [
    "Laravel activity",
    "Laravel activity logger",
    "Laravel activity log",
    "Laravel logger",
    "Laravel log"
  ],
  "license": "MIT",
  "type": "package",
  "authors": [
    {
      "name": "shintack",
      "email": "shintackeror404@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0.0",
    "jaybizzle/laravel-crawler-detect": "1.*",
    "laravelcollective/html": "^5.4|^5.5|^5.6|^5.7|^5.8|^6.0"
  },
  "autoload": {
    "psr-4": {
      "shintack\\LaravelLogger\\": "src/"
    },
    "files": [
      "src/app/Logic/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "shintack\\LaravelLogger\\LaravelLoggerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
